{
  "extends": "@strapi/typescript-utils/tsconfigs/server",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "jsx": "react",
    "module": "Node16",
    "moduleResolution": "Node16",
    "baseUrl": "."
  },
  "include": [
    "./",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.js"
  ],
  "exclude": [
    "node_modules/",
    "build/",
    "dist/",
    ".cache/",
    ".tmp/",
    "admin/",
    "**/*.test.ts"
  ]
}
